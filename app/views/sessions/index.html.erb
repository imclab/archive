<h3>All Sessions
  <%= render :partial => 'sorting' %>
</h3>
<% cache('all_sessions_with_songs') do %>
  <% @sessions.each do |session| %>
    <div class="session">
      <h4>
        <i class="icon-folder-open"></i>
        <%= prettify_date(session.session_date) %>
      </h4>
      <%= render partial: 'shared/song_list', locals: { songs: session.songs } %>
    </div>
  <% end %>
<% end %>
